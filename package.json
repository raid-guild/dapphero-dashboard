    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.14.1",
    "@types/styled-components": "^5.1.4",
    "copyfiles": "^2.4.0",
